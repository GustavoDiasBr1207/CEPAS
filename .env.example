# Exemplo de variáveis de ambiente para execução com Docker / local
# Copie este arquivo para `.env` e ajuste os valores.

# Banco de dados Oracle
DB_USER=ADMIN
DB_PASS=CepasDatabase@2025
DB_CONN=cepasdb_high

# Instant Client (caso queira sobrescrever o path no container)
INSTANT_CLIENT_LIB=/usr/src/app/instant

# Pool de conexões
DB_POOL_MIN=10
DB_POOL_MAX=10
DB_POOL_INCREMENT=0
DB_POOL_TIMEOUT=60

# Frontend API URL (quando não usando Docker Compose)
REACT_APP_API_BASE_URL=http://localhost:3001/api
